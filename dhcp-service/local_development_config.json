{
  "Dhcp4": {
    "option-def": [
      {
        "name": "delivery-optimisation",
        "code": 234,
        "type": "string",
        "space": "dhcp4"
      }
    ],
    "interfaces-config": {
      "interfaces": [
        "*"
      ],
      "dhcp-socket-type": "udp",
      "outbound-interface": "use-routing"
    },
    "lease-database": {
      "type": "mysql",
      "name": "<DB_NAME>",
      "user": "<DB_USER>",
      "password": "<DB_PASS>",
      "host": "<DB_HOST>",
      "port": 3306
    },
    "valid-lifetime": 86400,
    "host-reservation-identifiers": [
      "circuit-id",
      "hw-address",
      "duid",
      "client-id"
    ],
    "hosts-database": {
      "type": "mysql",
      "name": "<DB_NAME>",
      "user": "<DB_USER>",
      "password": "<DB_PASS>",
      "host": "<DB_HOST>",
      "port": 3306
    },
    "control-socket": {
      "socket-type": "unix",
      "socket-name": "/tmp/dhcp4-ctrl-socket"
    },
    "shared-networks": [
      {
        "name": "FITS_0001-1",
        "subnet4": [
          {
            "pools": [
              {
                "pool": "172.1.0.10 - 172.1.0.100"
              }
            ],
            "subnet": "172.1.0.0/24",
            "id": 1001,
            "user-context": {
              "site-id": "FITS_0001",
              "site-name": "Test"
            },
            "reservations": [
              {
                "hw-address": "aa:bb:cc:dd:ee:ff",
                "ip-address": "172.1.0.99",
                "hostname": "host.test.example.com",
                "user-context": {
                  "description": "Test Host"
                }
              }
            ],
            "require-client-classes": [
              "subnet-172.1.0.0-client"
            ]
          },
          {
            "pools": [
              {
                "pool": "10.1.0.1 - 10.1.0.254"
              }
            ],
            "subnet": "10.1.0.0/24",
            "id": 1002,
            "user-context": {
              "site-id": "FITS_0001",
              "site-name": "Test"
            },
            "require-client-classes": [
              "subnet-10.1.0.0-client"
            ]
          }
        ]
      }
    ],
    "subnet4": [
      {
        "pools": [
          {
            "pool": "127.0.0.1 - 127.0.0.254"
          }
        ],
        "subnet": "127.0.0.1/24",
        "id": 1
      }
    ],
    "loggers": [
      {
        "name": "kea-dhcp4",
        "output-options": [
          {
            "output": "stdout"
          }
        ],
        "severity": "DEBUG",
        "debuglevel": 99
      }
    ],
    "hooks-libraries": [
      {
        "library": "/usr/lib/kea/hooks/libdhcp_lease_cmds.so"
      },
      {
        "library": "/usr/lib/kea/hooks/libdhcp_stat_cmds.so"
      },
      {
        "library": "/usr/lib/kea/hooks/libdhcp_ha.so",
        "parameters": {
          "high-availability": [
            {
              "this-server-name": "<SERVER_NAME>",
              "mode": "hot-standby",
              "heartbeat-delay": 10000,
              "max-response-delay": 60000,
              "max-ack-delay": 10000,
              "max-unacked-clients": 0,
              "multi-threading": {
                        "enable-multi-threading": true,
                        "http-dedicated-listener": true,
                        "http-listener-threads": 0,
                        "http-client-threads": 0
              },
              "peers": [
                {
                  "name": "primary",
                  "url": "http://<PRIMARY_IP>:8000/",
                  "role": "primary",
                  "auto-failover": true
                },
                {
                  "name": "standby",
                  "url": "http://<STANDBY_IP>:8000/",
                  "role": "standby",
                  "auto-failover": true
                }
              ]
            }
          ]
        }
      }
    ],
    "multi-threading": {
      "enable-multi-threading": true,
      "thread-pool-size": 12,
      "packet-queue-size": 65
    },
    "option-data": [
      {
        "name": "domain-name",
        "data": "test_default.example.com"
      },
      {
        "name": "domain-name-servers",
        "data": "8.8.8.8, 8.8.4.4"
      }
    ],
    "client-classes": [
      {
        "name": "Test Client Class",
        "test": "option[77].hex == 'W10TEST'",
        "option-data": [
          {
            "name": "domain-name",
            "data": "test_client_class.example.com"
          }
        ]
      },
      {
        "name": "subnet-172.1.0.0-client",
        "test": "member('ALL')",
        "only-if-required": true,
        "option-data": [
          {
            "name": "routers",
            "data": "172.1.0.10"
          },
          {
            "name": "delivery-optimisation",
            "space": "dhcp4",
            "code": 234,
            "data": "a2f68131-2b7b-42f7-8fe8-1b6834a586d3"
          }
        ]
      },
      {
        "name": "subnet-10.1.0.0-client",
        "test": "member('ALL')",
        "only-if-required": true,
        "option-data": [
          {
            "name": "routers",
            "data": "10.1.0.10"
          },
          {
            "name": "delivery-optimisation",
            "space": "dhcp4",
            "code": 234,
            "data": "a2f68131-2b7b-42f7-8fe8-1b6834a586d3"
          }
        ]
      }
    ]
  }
}
