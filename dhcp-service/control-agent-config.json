{
  "Control-agent": {
    "http-host": "127.0.0.1",
    "http-port": 8001,
    "cert-required": false,
    "control-sockets": {
        "dhcp4": {
            "comment": "main server",
            "socket-type": "unix",
            "socket-name": "/tmp/dhcp4-socket"
        }
    },

    "loggers": [
    {
      "name": "kea-ctrl-agent",
      "severity": "INFO"
    }]
  }
}
